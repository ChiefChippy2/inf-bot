{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
      "max-len": ["warn", {
        "ignoreStrings": true,
        "code": 120
        }],
        "no-undef": "error"
    }
}
